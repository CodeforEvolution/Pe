
SubDir TOP Languages Sources ;

SubDirHdrs $(TOP) Sources ;

if $(OSPLAT) = X86 {
	C++FLAGS += -include [ FDirName $(TOP) Languages Sources CLangIncl.h ] -Wno-unused ;
} else {
	C++FLAGS += -prefix CLangIncl.h -w nounusedvar ;
}

# <pe-src>
PeLanguage AntlrExt 
	: CLang_antlr.cpp CantlrPopup.cpp
	: be 
	: keywords.g.rsrc 
	;
# </pe-src>

